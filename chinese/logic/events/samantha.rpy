    # ch_Name "It's hard to say."
    ch_Name "这很难说。"

# game/logic/events/samantha.rpy:1824
translate chinese Samantha_12_label_40d651f4:

    # ch_Name "Her beauty is undeniable, but..."
    ch_Name "她的美丽无可否认，但是。。。"

# game/logic/events/samantha.rpy:1826
