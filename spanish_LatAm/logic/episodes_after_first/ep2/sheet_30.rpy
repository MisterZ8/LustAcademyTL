    # ch_Name "{i}(Wow! So this is our dorm?){/i}"
    ch_Name "{i}(¡Vaya! ¿Así que este es nuestro dormitorio?){/i}"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:33
translate spanish_LatAm sheet_30_c7d7472a:

    # ch_Name "{i}(Looks much more modern than I expected.){/i}"
    ch_Name "{i} (Se ve mucho más moderna de lo que esperaba.) {/i}"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:35
translate spanish_LatAm sheet_30_14e32c59:

    # Elijah "Welcome to Leonheart, kids! "
    Elijah "¡Bienvenidos a Leonheart, chicos!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:36
translate spanish_LatAm sheet_30_f9c5a6d1:

    # Elijah "Best conditions for best students!"
    Elijah "¡Las mejores condiciones para los mejores estudiantes!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:37
translate spanish_LatAm sheet_30_0b357859:

    # Elijah "Make room!"
    Elijah "¡Hagan espacio!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:38
translate spanish_LatAm sheet_30_134f228b:

    # Elijah "Choose any free room for yourself. This is your home now!"
    Elijah "Elijan cualquier habitación libre para usted. ¡Esta es tu casa ahora!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:39
translate spanish_LatAm sheet_30_cef0c6ef:

    # Elijah "But the main place in our house is this living room. Here..."
    Elijah "¡Pero el lugar principal de nuestra casa es esta sala de estar. Aquí..."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:40
translate spanish_LatAm sheet_30_e8fa5614:

    # Elijah "...is where real magic happens!"
    Elijah "...es donde sucede la verdadera magia!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:41
translate spanish_LatAm sheet_30_8b95d622:

    # Elijah "Parties, get-togethers, live communication..."
    Elijah "Fiestas, reuniones, comunicación en directo..."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:42
translate spanish_LatAm sheet_30_cd2d528a:

    # Elijah "Celebrating house victories in tournaments and competitions, orgies..."
    Elijah "Celebrando las victorias de la casa en torneos y competiciones, orgías..."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:43
translate spanish_LatAm sheet_30_58d7675b:

    # Elijah "Well, there haven't really been any orgies yet, but hey!"
    Elijah "Bueno, en realidad no ha habido ninguna orgía todavía, pero ¡oye!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:44
translate spanish_LatAm sheet_30_2c1c6663:

    # Elijah "You are new blood. Pioneers. I believe in you."
    Elijah "Ustedes son sangre nueva. pioneros. Creo en ustedes."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:45
translate spanish_LatAm sheet_30_5671d544:

    # Elijah "Just let me know in advance so that I can clear my plans for the day."
    Elijah "Solo házmelo saber con anticipación para que pueda aclarar mis planes para el día."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:47
translate spanish_LatAm sheet_30_32d53f3d:

    # Elijah "So. What have I forgotten... Oh yes. Do any of you have questions for me?"
    Elijah "Asi que. ¿Qué he olvidado? Oh, sí. ¿Alguno de ustedes tiene preguntas para mí?"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:67
translate spanish_LatAm sheet_30_menu_7472c685:

    # ch_Name "I don't see a bar here, how can we party without booze?"
    ch_Name "No veo un bar aquí, ¿cómo podemos divertirnos sin alcohol?"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:69
translate spanish_LatAm sheet_30_menu_a20f54f7:

    # Elijah "The booze is all right here, trust me! We have our own sources."
    Elijah "¡La bebida está bien aquí, confía en mí! Tenemos nuestras propias fuentes."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:70
translate spanish_LatAm sheet_30_menu_4a010f54:

    # Elijah "You just have to live long enough to see our parties!"
    Elijah "¡Solo tienes que vivir lo suficiente para ver nuestras fiestas!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:72
translate spanish_LatAm sheet_30_menu_06981438:

    # Elijah "And after that, you can die in peace!"
    Elijah "¡Y después de eso, puedes morir en paz!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:77
translate spanish_LatAm sheet_30_menu_answ_2_a244ffa2:

    # ch_Name "When is the best time to go to bed? "
    ch_Name "¿Cuándo es el mejor momento para ir a la cama?"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:79
translate spanish_LatAm sheet_30_menu_answ_2_236d0644:

    # ch_Name "What time do classes begin?"
    ch_Name "¿A qué hora comienzan las clases?"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:81
translate spanish_LatAm sheet_30_menu_answ_2_c3fa66cd:

    # Elijah "Good question! Boring, but really important information:"
    Elijah "¡Buena pregunta! Información aburrida, pero realmente importante:"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:82
translate spanish_LatAm sheet_30_menu_answ_2_c7bb271a:

    # Elijah "Your schedule and all news are sent directly to your mobile phone."
    Elijah "Su horario y todas las noticias se envían directamente a su teléfono móvil."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:84
translate spanish_LatAm sheet_30_menu_answ_2_96e798ee:

    # ch_Name "How does that work?"
    ch_Name "¿Cómo funciona?"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:85
translate spanish_LatAm sheet_30_menu_answ_2_dfad5c35:

    # Elijah "If you want to make something of yourself here — you will attend classes."
    Elijah "Si quieres hacer algo de ti mismo aquí, asistirás a clases."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:86
translate spanish_LatAm sheet_30_menu_answ_2_7306a082:

    # Elijah "And if you want to fail your training and get expelled — it's up to you."
    Elijah "Y si quieres fallar en tu entrenamiento y ser expulsado, depende de ti."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:88
translate spanish_LatAm sheet_30_menu_answ_2_676a3bd6:

    # Elijah "I advise everyone to go to bed early and get a good night's sleep."
    Elijah "Aconsejo a todos que se acuesten temprano y duerman bien."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:89
translate spanish_LatAm sheet_30_menu_answ_2_b5331d41:

    # Elijah "The magical energy that charged you up on the train is about to run out..."
    Elijah "La energía mágica que te cargó en el tren está a punto de agotarse..."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:90
translate spanish_LatAm sheet_30_menu_answ_2_ea52561a:

    # Elijah "...and no one likes to drag students... "
    Elijah "... y a nadie le gusta arrastrar a los estudiantes ..."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:91
translate spanish_LatAm sheet_30_menu_answ_2_599c22de:

    # Elijah "...who have passed out in the middle of the room to their bedrooms."
    Elijah "...que se han desmayado en medio de la habitación a sus dormitorios."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:95
translate spanish_LatAm sheet_30_menu_answ_3_9b4abf1c:

    # ch_Name "What victories were you talking about? "
    ch_Name "¿De qué victorias estabas hablando?"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:96
translate spanish_LatAm sheet_30_menu_answ_3_47e05278:

    # ch_Name "Who is our house competing with?"
    ch_Name "¿Con quién compite nuestra casa?"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:98
translate spanish_LatAm sheet_30_menu_answ_3_de295e21:

    # Elijah "Great question! With other houses, of course!"
    Elijah "¡Buena pregunta! ¡Con otras casas, por supuesto!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:99
translate spanish_LatAm sheet_30_menu_answ_3_39cc4c2b:

    # Elijah "The best house in Cordale is selected each week."
    Elijah "Cada semana se selecciona la mejor casa de Cordale."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:100
translate spanish_LatAm sheet_30_menu_answ_3_58559787:

    # Elijah "It depends on how fun and rowdy you spend your weekend."
    Elijah "Depende de lo divertido y ruidoso que pases tu fin de semana."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:101
translate spanish_LatAm sheet_30_menu_answ_3_a05ff41a:

    # Elijah "You are probably impressed by the academy, but! "
    Elijah "Probablemente estés impresionado por la academia, ¡pero!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:102
translate spanish_LatAm sheet_30_menu_answ_3_67008087:

    # Elijah "Only ghosts like to hang around these walls for a whole year."
    Elijah "Solo a los fantasmas les gusta andar por estas paredes durante todo un año."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:103
translate spanish_LatAm sheet_30_menu_answ_3_fe89c76b:

    # Elijah "And that's not a fact. I don't think they have a choice. But we do!"
    Elijah "Y eso no es un hecho. No creo que tengan elección. ¡Pero lo hacemos!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:104
translate spanish_LatAm sheet_30_menu_answ_3_d4258485:

    # Elijah "Collect the most points — have fun in Dale!"
    Elijah "Acumule la mayor cantidad de puntos: ¡diviértase en Dale!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:105
translate spanish_LatAm sheet_30_menu_answ_3_985de653:

    # Elijah "It's not saying much but trust me — it's worth being ready for class!"
    Elijah "No dice mucho, pero créeme, ¡vale la pena estar listo para la clase!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:109
translate spanish_LatAm end_sheet_30_8d099b3e:

    # Elijah "Thank you all for your attention! Rest now, you were all great today."
    Elijah "¡Gracias a todos por su atención! Descansa ahora, estuviste genial hoy."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:111
translate spanish_LatAm end_sheet_30_89a19454:

    # Elijah "[Name], I see you are an active guy. "
    Elijah "[Name], veo que eres un chico activo."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:113
translate spanish_LatAm end_sheet_30_b399c11f:

    # Elijah "Come to the living room in the morning before class!"
    Elijah "¡Ven a la sala de estar por la mañana antes de clase!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:114
translate spanish_LatAm end_sheet_30_5668d023:

    # Elijah "I have some business for you."
    Elijah "Tengo algunos asuntos para ti."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:115
translate spanish_LatAm end_sheet_30_445242b7:

    # ch_Name "Okay, of course."
    ch_Name "Está bien, por supuesto."

# game/logic/episodes_after_first/ep2/sheet_30.rpy:116
translate spanish_LatAm end_sheet_30_178e9f31:

    # Elijah "Sweet! C'ya!"
    Elijah "¡Oki doki! ¡Adios!"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:118
translate spanish_LatAm end_sheet_30_76381c03:

    # ch_Name "{i}(This guy seems great.){/i}"
    ch_Name "{i}(Este tipo parece genial).{/i}"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:119
translate spanish_LatAm end_sheet_30_cbf0a58e:

    # ch_Name "{i}(I hope my gut isn't lying about him.){/i}"
    ch_Name "{i}(Espero que mi instinto no esté mintiendo sobre él).{/i}"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:120
translate spanish_LatAm end_sheet_30_4b25adde:

    # ch_Name "{i}(Alright. Where's my room?){/i}"
    ch_Name "{i}(Está bien. ¿Dónde está mi habitación?){/i}"

# game/logic/episodes_after_first/ep2/sheet_30.rpy:121
translate spanish_LatAm end_sheet_30_1a1af4d7:

